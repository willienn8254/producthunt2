<h1>Productos</h1>


<table>
	
	<tr>
		<th>Id</th>
		<th>Nombre</th>
		<th>Url</th>
		<th>Descripcion</th>


	</tr>

	<% @products.each do |product| %>

	<tr>

		<td><%=product.id %></td>
		<td><%=product.name %></td>
		<td><%=product.url %></td>
		<td><%=product.description %></td>
		




	</tr>



	<% end %>


</table>